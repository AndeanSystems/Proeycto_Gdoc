        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">Nueva Mesa Virtual</h3>
                        <button class="btn btn-default pull-right" ng-click="ctrl.CambiarVentana('List')"><i class="fa fa-list-alt" data-placement="top" data-toggle="tooltip" title="Lista"></i></button>
                        <button class="btn btn-default pull-right"><i class="fa fa-users" data-placement="top" data-toggle="tooltip" title="Compartir"></i></button>
                        <button class="btn btn-default pull-right"><i class="fa fa-floppy-o" data-placement="top" data-toggle="tooltip" title="Guardar"></i></button>
                    </div>

                    <div class="table-responsive mailbox-messages">
                            <table class="table table-hover table-striped">
                                <tbody>
                                    <tr>
                                        <td>&nbsp;</td>
                                    </tr>
                                    <div class="mailbox-controls">


                                    </div>
                                    <div class="table-responsive mailbox-messages">
                                        <div class="col-sm-12">
                                            <div class="col-sm-4">
                                                <div>
                                                    <div class="form-group">
                                                        <label class="col-sm-12">Tipo de Accesso </label>
                                                        <md-radio-group ng-model="ctrl.operacion.AccesoOperacion">
                                                            <md-radio-button style="display:inline-block;margin-right:2em;margin-left:1em;" value="{{ta.CodiConcepto}}" class="standard" ng-repeat="ta in ctrl.listTipoAcceso | limitTo:2">{{ta.DescripcionConcepto}} </md-radio-button>
                                                        </md-radio-group>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label class="col-sm-12">Tipo Mesa Virtual</label>
                                                    <div class="col-sm-12">
                                                        <select class="form-control" ng-model="ctrl.operacion.TipoMesaVirtual" ng-options="td.CodiConcepto as td.DescripcionConcepto for td in ctrl.listTipoMesaVirtual">
                                                            <option value="">Seleccione</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label class="col-sm-12">Prioridad Atencion</label>
                                                    <div class="col-sm-12">
                                                        <select class="form-control" ng-model="ctrl.operacion.TipoMesaVirtual" ng-options="td.CodiConcepto as td.DescripcionConcepto for td in ctrl.listPrioridadAtencion">
                                                            <option value="">Seleccione</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>



                                        <div class="col-sm-12">
                                            <div class="col-sm-4">

                                                <div class="form-group">
                                                    <label class="col-sm-12">Fecha de Apertura </label>
                                                    <div class="col-sm-12">
                                                        <div class="input-group date">
                                                            <input type="text" class="form-control"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                                                        </div>  
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label class="col-sm-12">Fecha de Cierre</label>
                                                    <div class="col-sm-12">
                                                        <div class="input-group date">
                                                            <input type="text" class="form-control"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label class="col-sm-12">Notificar Acciones</label>
                                                    <md-radio-group ng-model="ctrl.operacion.Derivacion">
                                                        <md-radio-button style="display:inline-block;margin-right:5em;margin-left:1em;" value="0" class="standard">Si</md-radio-button>
                                                        <md-radio-button style="display:inline-block;margin-right:2em;margin-left:1em;" value="1" class="standard">No</md-radio-button>

                                                    </md-radio-group>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-12">

                                            <div class="col-sm-8">
                                                <div class="form-group">
                                                    <label class="col-sm-12">Organizador </label>
                                                    <div class="col-sm-12">
                                                        <input type="text" class="form-control" placeholder="JSANCHEZ">
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                        <div class="col-sm-12">
                                            <div class="col-sm-8">
                                                <div class="form-group">
                                                    <label class="col-sm-12">Titulo</label>
                                                    <div class="col-sm-12">
                                                        <input type="text" class="form-control" placeholder="Titulo">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-12">
                                            <div class="col-sm-8">
                                                <div class="form-group">
                                                    <label class="col-sm-12">Asunto</label>
                                                    <div class="col-sm-12">
                                                        <input type="text" class="form-control" placeholder="Asunto">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-12">
                                            <div class="col-sm-8">
                                                <div class="form-group">
                                                    <label class="col-sm-12">Invitados</label>
                                                    <div class="col-sm-12">
                                                        <md-content class="autocomplete" layout="column">
                                                            <md-contact-chips ng-model="ctrl.usuarioDestinatarios"
                                                                                md-contacts="ctrl.querySearch($query)"
                                                                                md-contact-name="Nombre"
                                                                                md-highlight-flags="i"
                                                                                filter-selected="ctrl.filterSelected"
                                                                                placeholder="Destinatario">
                                                            </md-contact-chips>
                                                        </md-content>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-sm-12">
                                            <div class="col-sm-8">
                                                <div>
                                                    <div class="btn-group">
                                                        <span> &nbsp;  </span>
                                                    </div>Documento Adjunto
                                                    <!-- /.btn-group -->
                                                    <input type="file" class="form-control btn-file btn-default" id="Email4">

                                                </div>
                                            </div>
                                        </div>


                                    </div>
                                    <tr>
                                        <td> &nbsp; </td>
                                    </tr>

                                </tbody>
                            </table>
                            <!-- /.table -->
                        </div>
                        
                </div>
            </div>
        </div>