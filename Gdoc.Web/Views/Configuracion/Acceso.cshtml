<!-- Content Header (Page header) -->
<section class="content-header">
    <section class="content">
        <!--comienso de cuerpo documento electrocnico-->
        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary margin-top">
                    <div class="box-header with-border">
                        <h3 class="box-title">Accesos</h3>
                    </div>
                    <div class="box-body no-padding">

                        <div class="row">
                            <br>

                            <div class="col-md-2">
                                <h5 class="pull-right">Usuario</h5>
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="form-control" placeholder="Buscar Usuario">
                            </div>

                            <div class="col-md-2">
                                <button type="submit" name="search" id="search-btn" class="btn btn-default"><i class="fa fa-search" data-placement="top" data-toggle="tooltip" title="Buscar"></i></button>
                                <button class="btn btn-default"><i class="fa fa-floppy-o" data-placement="top" data-toggle="tooltip" title="Guardar"></i></button>
                                @*<button class="btn btn-default" data-toggle="modal" data-target="#modal_contenido"><i class="fa fa-file fa-1x" data-placement="top" data-toggle="tooltip" title="Nuevo"></i></button>*@
                            </div>
                        </div>





                        <br>


                    </div>

                    <div class="table-responsive mailbox-messages">
                        <table class="table table-hover table-striped">
                            <thead>
                            <th>Pagina<i class="fa fa-sort"></i></th>
                            <th>DireccionUrl<i class="fa fa-sort"></i></th>
                            <th>Habilitar<i class="fa fa-sort"></i></th>

                            </thead>
                            <tbody>
                                <tr>
                                    <td class="mailbox-date">Escritorio Virtual</td>
                                    <td class="mailbox-subject">../Inicio/frmEscritorioVirtual.aspx</td>
                                    <td class="mailbox-subject"><input type="checkbox" style="opacity:initial" value=""></td>
                                </tr>
                                <tr>
                                    <td class="mailbox-date">Cambiar Contraseña</td>
                                    <td class="mailbox-subject">../Inicio/frmCambiarClave.aspx</td>
                                    <td class="mailbox-subject"><input type="checkbox" style="opacity:initial" value=""></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>


                </div>

            </div>
        </div>

        <!--modal de editor-->
        <div class="modal fade" id="modal_contenido">
            <div class="modal-dialog modal-lg" style="width:60%;">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title">Registro Concepto</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="box box-primary">
                                    <div class="box-header with-border">
                                        <h3 class="box-title">Opciones del Concepto</h3>
                                    </div><!-- /.box-header -->
                                    <!-- form start -->
                                    <form role="form">
                                        <div class="box-body no-padding">

                                            <div class="form-group no-margin-bottom">
                                                <label>Tipo de Concepto</label>
                                                <select class="form-control">
                                                    <option>Estado</option>
                                                    <option>Mensaje</option>
                                                </select>
                                            </div>

                                            <div class="form-group no-margin-bottom">
                                                <label>Codigo</label>
                                                <input type="text" class="form-control" placeholder="Codigo">
                                            </div>

                                            <div class="form-group no-margin-bottom">
                                                <label>Descripcion</label>
                                                <input type="text" class="form-control" placeholder="Descripcion">
                                            </div>

                                            <div class="form-group no-margin-bottom">
                                                <label>Descripcion Abreviatura</label>
                                                <input type="text" class="form-control" placeholder="Abreviatura">
                                            </div>

                                            <div class="form-group no-margin-bottom">
                                                <label class="col-sm-4">Estado</label>
                                                <div class="col-sm-8">
                                                    <label><input type="checkbox"> Activo</label>
                                                </div>
                                            </div>


                                        </div>
                                    </form>
                                </div>
                            </div><!-- /.col -->

                            <div class="col-md-6">
                                <div class="box box-primary">
                                    <div class="box-header with-border">
                                        <h3 class="box-title">Contenido del Concepto</h3>
                                        <button class="btn btn-default pull-right"><span class="glyphicon glyphicon-floppy-saved" data-placement="top" data-toggle="tooltip" title="Guardar enviar"></span></button>
                                        <button class="btn btn-default pull-right"><i class="fa fa-floppy-o" data-placement="top" data-toggle="tooltip" title="Guardar"></i></button>
                                    </div><!-- /.box-header -->

                                    <form role="form">
                                        <div class="box-body no-padding">

                                            <div class="form-group no-margin-bottom">
                                                <label>Valor1</label>
                                                <input type="text" class="form-control" placeholder="Valor1">
                                            </div>

                                            <div class="form-group no-margin-bottom">
                                                <label>Valor2</label>
                                                <input type="text" class="form-control" placeholder="Valor2">
                                            </div>

                                            <div class="form-group no-margin-bottom">
                                                <label>Texto1</label>
                                                <input type="text" class="form-control" placeholder="Texto1">
                                            </div>

                                            <div class="form-group no-margin-bottom">
                                                <label>Texto2</label>
                                                <input type="text" class="form-control" placeholder="Texto2">
                                            </div>


                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="modal-footer">
                            </div><!-- /.modal-content -->
                        </div><!-- /.modal-dialog -->
                    </div><!-- /.modal -->
                </div>
            </div>
        </div>

    </section>


</section>
