    <!--<script src="dist/js/demo.js" type="text/javascript"></script>-->
    <script src="~/Content/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js" type="text/javascript"></script>
    <!--SweetAlert-->
    <script src="~/Content/plugins/sweetalert/sweetalert.min.js"></script>
    <script src="~/Content/plugins/filestyle/bootstrap-filestyle.min.js"></script>
    <script>
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>

    <style>
        span.select2 {
            width: 100% !important;
        }
        .sidebar-menu li > a {
            position: relative;
            font-size: 13px;
        }
        .sidebar-menu .treeview-menu > li > a {
            padding: 5px 5px 5px 15px;
            display: block;
            font-size: 13px;
        }
        .modal-content {
            border-radius: 5px;
        }
        label {
            font-family: 'Source Sans Pro',sans-serif;
            font-size: 13px;
            font-weight: normal;
            line-height: 2;
        }
        body {
            font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
            font-size: 12px;
        }
        .form-control {
            font-size: 12px;
        }

        .no-margin-bottom {
            margin-bottom: 0px !important;
        }
    </style>
    <!--Estilos Controles Angular-->
    <style>
        .md-chips {
            display: block;
            font-family: Roboto,Helvetica Neue,sans-serif;
            font-size: 12px;
            padding: 0 0 8px 3px;
            vertical-align: middle;
        }

        md-icon {
            min-height: 20px;
            min-width: 20px;
        }
        md-radio-button {
            box-sizing: border-box;
            display: block;
            margin-bottom: 16px;
            white-space: nowrap;
            cursor: pointer;
            position: relative;
            font-size: 10px;
        }
        md-radio-button.md-default-theme.md-checked .md-off, md-radio-button.md-checked .md-off {
            border-color: #E83333;
        }

        md-radio-button.md-default-theme .md-on, md-radio-button .md-on {
            background-color: #E83333;
        }
    </style>
</head>
<body class="skin-blue sidebar-mini" style="background-color: #ecf0f5" ng-app="app">
    <div class="wrapper">
        <header class="main-header">
            @Html.Partial("_Header")
        </header>
        <div>
            @Html.Partial("_Sidebar")
        </div>
        <div class="content-wrapper">

            @RenderBody()

        </div>
        <div>

        </div>

        <footer class="main-footer">
            @Html.Partial("_Footer")
        </footer>

        <!--modal en construccion-->
        <div class="modal fade" id="modal_page" tabindex="-1">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    </div>
                    <div class="modal-body">
                        <h2>En construcción</h2>
                    </div>
                </div>
            </div>
        </div>

        <div id="modal_imagenes" class="modal fade">
            <div class="modal-dialog modal-lg" style="width:25%;">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title">Seleccione su avatar</h4>
                    </div>
                    <div class="modal-body">
                        <div class="box box-body">
                            <div class="col-md-12 margin-bottom">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/1.png" style="width:auto; height:70px;" class="img-responsive col-md-4">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/2.png" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/3.png" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                            </div>
                            <div class="col-md-12 margin-bottom">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/4.png" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/5.jpg" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/6.png" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                            </div>
                            <div class="col-md-12 margin-bottom">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/7.jpg" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/8.png" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/9.png" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                            </div>
                            <div class="col-md-12 margin-bottom">
                                <img src="http://192.168.100.29:85/IMAGENES/avatars/incognito.png" style="width: auto; height: 70px;" class="img-responsive col-md-4">
                            </div>
                            
                            
                            
                        </div>
                        <div class="modal-footer">
                            <input type="file" class="filestyle" id="fotoAvatar" accept="image/*">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/scripts/angular/angular.min.js"></script>
    <script src="~/scripts/angular-locale_es-pe.js"></script>
    <script src="~/scripts/ui-grid.min.js"></script>
    <script src="~/scripts/angular-animate/angular-animate.min.js"></script>
    <script src="~/scripts/angular-aria/angular-aria.min.js"></script>
    <script src="~/scripts/angular-material/angular-material.min.js"></script>
    <script src="~/scripts/appjs/app-factory.js"></script>
    <script src="~/scripts/appjs/app-service.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
