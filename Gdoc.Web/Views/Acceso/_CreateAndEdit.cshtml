<div class="modal fade" id="modal_contenido">
    <div class="modal-dialog modal-lg" style="width:60%;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Accesos de Usuario</h4>
            </div>
            <div class="modal-body">
                <div class="row">

                    <div class="col-md-12">
                        <div class="box box-primary">
                            <div class="box-header with-border">
                                <h3 class="box-title">Accesos</h3>
                                @*<button class="btn btn-default pull-right"><span class="glyphicon glyphicon-floppy-saved" data-placement="top" data-toggle="tooltip" title="Guardar enviar"></span></button>*@
                                <button class="btn btn-default pull-right"><i class="fa fa-floppy-o" data-placement="top" data-toggle="tooltip" title="Guardar"></i></button>
                            </div><!-- /.box-header -->

                            <form role="form">
                                <div class="box-body no-padding">

                                    <div class="form-group no-margin-bottom" style="margin-top:5px">
                                        <label class="col-sm-12">Modulo de Sistema</label>
                                        <div class="col-sm-12">
                                            <input type="text" class="form-control" ng-model="ctrl.accesosistema.ModuloPaginaUrl.ModuloSistema" disabled="true">
                                        </div>
                                    </div>

                                    <div class="form-group no-margin-bottom" style="margin-top:5px">
                                        <label class="col-sm-12">Nombre de Pagina</label>
                                        <div class="col-sm-12">
                                            <input type="text" class="form-control" ng-model="ctrl.accesosistema.ModuloPaginaUrl.NombrePagina" disabled="true">
                                        </div>
                                    </div>

                                    <div class="form-group no-margin-bottom" style="margin-top:5px">
                                        <label class="col-sm-12">Direccion de Pagina</label>
                                        <div class="col-sm-12">
                                            <input type="text" class="form-control"  ng-model="ctrl.accesosistema.ModuloPaginaUrl.DireccionFisicaPagina" disabled="true">
                                        </div>
                                    </div>
                                    <table class="table table-hover table-striped">
                                        <thead>
                                        <th>Roles<i class="fa fa-sort"></i></th>
                                        <th>Habilitar<i class="fa fa-sort"></i></th>

                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td class="mailbox-date">consultar</td>
                                                <td class="mailbox-subject"><input type="checkbox" style="opacity:initial" value=""></td>
                                            </tr>
                                            <tr>
                                                <td class="mailbox-date">eliminar</td>
                                                <td class="mailbox-subject"><input type="checkbox" style="opacity:initial" value=""></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="modal-footer">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
