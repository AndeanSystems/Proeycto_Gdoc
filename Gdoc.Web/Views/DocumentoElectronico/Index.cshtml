<!-- Content Header (Page header) -->
<section class="content-header">
    <section class="content" ng-controller="documentoelectronico_controller as ctrl" layout="column" ng-cloak>
        <div class="row" ng-if="ctrl.visible == 'List'">
            @Html.Partial("_List")
        </div>
        <div class="row" ng-if="ctrl.visible == 'CreateAndEdit'">
            @Html.Partial("_CreateAndEdit")
        </div>
    </section>
</section>
@section Scripts{
    <script src="~/scripts/appjs/documentoelectronico-controller.js"></script>
}

