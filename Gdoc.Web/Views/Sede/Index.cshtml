<!-- Content Header (Page header) -->
<section class="content-header">
    <section class="content" ng-controller="sede_controller as ctrl" ng-cloak>
        <!--comienso de cuerpo documento electrocnico-->
        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary margin-top">
                    <div class="box-header with-border">
                        <div class="col-md-10">
                            <h3 class="box-title">Sedes</h3>
                        </div>

                        <button class="btn btn-default" style="right:auto" ng-click="ctrl.CambiarVentana()"><i class="fa fa-file fa-1x" data-placement="top" data-toggle="tooltip" title="Nuevo"></i></button>
                    </div>
                    <div class="box-body no-padding">


                        <br>

                    </div>

                    <div class="table-responsive mailbox-messages">
                        <div class="grid" style="font-size: 12px; height: 400px" ui-grid="ctrl.gridOptions" ui-grid-pagination ui-grid-auto-resize></div>
                     
                    </div>


                </div>

            </div>
        </div>

        <!--modal de editor-->
        @Html.Partial("_CreateAndEdit")



    </section>
</section>
@section Scripts{
    <script src="~/scripts/appjs/sede-controller.js"></script>
}
