<section class="content-header">

</section>
<section class="content" ng-controller="sede_controller as ctrl" ng-cloak>
    <!--comienso de cuerpo documento electrocnico-->
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Sedes</h3>
                    <button class="btn btn-default pull-right btn-sm" style="right:auto" ng-click="ctrl.CambiarVentana()"><i class="fa fa-file fa-1x" data-placement="top" data-toggle="tooltip" title="Nuevo"></i></button>
                </div>
                <div class="box-body no-padding">
                </div>
                <div class="grid" style="font-size: 12px; height: 400px" ui-grid="ctrl.gridOptions" ui-grid-pagination ui-grid-auto-resize ui-grid-resize-columns ui-grid-move-columns></div>
            </div>
        </div>
    </div>
    <!--modal de editor-->
    @Html.Partial("_CreateAndEdit")
</section>
@section Scripts{
    <script src="~/scripts/appjs/sede-controller.js"></script>
}
